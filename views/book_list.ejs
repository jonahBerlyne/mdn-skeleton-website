<!DOCTYPE html>
<html>
 <head>
  <title>
   <%= title %>
  </title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
 </head>
 <body>
   <%- include('./layout.ejs') %>
   <h1>
    <%= title %>
   </h1>
     <ul>
      <% if (book_list.length > 0) { %>
       <% book_list.forEach(book => { %>
        <li>
         <a href=<%- book.url %>><%= book.title %> | <%= book.author.name %></a>
        </li>
       <% }); %>
      <% } else { %>
       <li>
        <p>There are no books.</p>
       </li>
      <% } %>
     </ul>
 </body>
</html>