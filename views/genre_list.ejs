<!DOCTYPE html>
<html>
 <head>
  <title>
   <%= title %>
  </title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
 </head>
 <body>
  <%- include('./layout.ejs') %>
   <h1>
    <%= title %>
   </h1>
   <ul>
    <% if (genre_list.length> 0) { %>
     <% genre_list.forEach(genre => { %>
      <li>
       <a href=<%- genre.url %>>
        <%= genre.name %>
       </a>
      </li>
     <% }); %>
    <% } else { %>
        <li>
         <p>There are no genres.</p>
        </li>
    <% } %>
   </ul>
 </body>
</html>