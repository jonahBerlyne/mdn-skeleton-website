<!DOCTYPE html>
<html>
 <head>
  <title>
   <%= title %>
  </title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
 </head>
 <body>
  <%- include('./layout.ejs') %>
   <h1>
    <%= title %>
   </h1>
   <ul>
    <% if (author_list.length> 0) { %>
      <% author_list.forEach(author => { %>
       <% console.log(author) %>
       <li>
       <a href=<%- author.url %>>
        <%= author.name %> | <%= author.lifespan %>
       </a>
      </li>
      <% }); %>
     <% } else { %>
        <li>
         <p>There are no authors.</p>
        </li>
     <% } %>
   </ul>
 </body>
</html>